class Solution{
    public:
    string colName (long long int s)
    {
        string ans="";
        while(s)
        {
            if(s%26==0)
            {
                ans+='Z';
                s=s/26-1;
            }
            else
            {
                ans+=('A'-1+s%26);
                s/=26;
            }
        }
        reverse(ans.begin(),ans.end());
        return ans;
    }
};
